{
  "name": "react-litert",
  "description": "React integration for Googleâ€™s LiteRT.js",
  "packageManager": "yarn@4.11.0",
  "version": "0.1.1",
  "type": "module",
  "author": "Cedric Mutesa <mutesacedric@gmail.com>",
  "engines": {
    "node": ">=16.6.0"
  },
  "keywords": [
    "WebAI",
    "litert",
    "google",
    "lite runtime",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mutesa-Cedric/react-litert.git"
  },
  "bugs": {
    "url": "https://github.com/Mutesa-Cedric/react-litert/issues"
  },
  "homepage": "https://github.com/Mutesa-Cedric/react-litert#readme",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/types/index.d.cts",
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/types/core.d.mts",
        "default": "./dist/esm/core.mjs"
      },
      "require": {
        "types": "./dist/types/core.d.cts",
        "default": "./dist/cjs/core.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "generate-dts": "tsx scripts/generate-dts",
    "build": "rollup -c rollup.config.js && npm run generate-dts",
    "pack:test": "tsx scripts/pack-test",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "jest": "jest --passWithNoTests",
    "test": "npm run prettier:check && npm run typecheck && npm run lint && npm run jest && npm run build && npm run pack:test",
    "release": "npm test && tsx scripts/release",
    "knip": "knip",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/fs-extra": "^11",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.3",
    "@types/react": "^19.2.6",
    "@types/signale": "^1",
    "@types/yargs": "^17.0.33",
    "chalk": "^5.6.2",
    "esbuild": "^0.26.0",
    "eslint": "^9.35.0",
    "fs-extra": "^11.3.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "knip": "^5.63.1",
    "new-github-release-url": "^2.0.0",
    "open": "^10.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-node-externals": "^8.1.1",
    "signale": "^1.4.0",
    "simple-git": "^3.28.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "version-next": "^1.0.2",
    "yargs": "^18.0.0",
    "zx": "^8.8.1"
  },
  "dependencies": {
    "@litertjs/core": "^0.2.1",
    "@litertjs/tfjs-interop": "^1.0.1",
    "@tensorflow/tfjs-core": "^4.22.0"
  },
  "peerDependencies": {
    "react": "*"
  }
}
